	"item": [
		{
			"name": "根据用户名密码获取token",
			"event": [
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"tests[\"状态200\"] = responseCode.code === 200;",
							"tests[\"has token\"] = responseBody.has(\"token\");",
							"var jsonData = JSON.parse(responseBody);",
							"postman.setEnvironmentVariable(\"Bearer\", jsonData.result.token);",
							"",
							"//跳转到 Request3 接口",
							"//postman.setNextRequest(\"Request3\");"
						]
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"tenancyName\": \"{{tenancyName}}\",\r\n  \"username\": \"{{username}}\",\r\n  \"password\": \"{{psw}}\"\r\n}"
				},
				"url": {
