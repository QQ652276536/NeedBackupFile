{
	"info": {
		"_postman_id": "70297e76-0ec4-436a-821f-674cd7290037",
		"name": "根据用户名获取Token(测试前先运行一次)",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "http://192.168.99.228:6100/api/Account",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"tests[\"状态200\"] = responseCode.code === 200;"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "authorization",
						"value": "Bearer {{Bearer}}"
					},
					{
						"key": "content-type",
						"value": "application/json"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"tenancyName\": \"{{tenancyName}}\",\r\n  \"username\": \"{{username}}\",\r\n  \"password\": \"{{psw}}\"\r\n}"
				},
				"url": {
					"raw": "{{url}}/api/Account",
					"host": [
						"{{url}}"
					],
					"path": [
						"api",
						"Account"
					]
				}
			},
			"response": []
		}
	]
}